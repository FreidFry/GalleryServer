{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=Data/AppDbContext.db"
  },
  "FileStorage": {
    "UserDataBasePath": "Data/UsersData",
    "GalleryFolderName": "Gallery",
    "ProfileFolderName": "Profile",
